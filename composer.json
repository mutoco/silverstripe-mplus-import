{
    "name": "mutoco/silverstripe-mplus-import",
    "description": "Import data-records from MuseumPlus into SilverStripe.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">= 7.3, <8",
        "ext-libxml": "*",
        "ext-dom": "*",
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "symbiote/silverstripe-queuedjobs": "^4.7.0",
        "php-http/guzzle7-adapter": "^1.0.0",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "silverstripe/sqlite3": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Mutoco\\Mplus\\": "src/",
            "Mutoco\\Mplus\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
